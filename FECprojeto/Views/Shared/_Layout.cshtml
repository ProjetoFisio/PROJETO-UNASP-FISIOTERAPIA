<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/content/inicio/index.css" rel="stylesheet" />
    <link href="~/content/inicio/inicial.css" rel="stylesheet" />
    <link href="~/content/inicio/style.css" rel="stylesheet" />
    <script src="~/scripts/inicio/inicial.js"></script>
</head>
<body>
    <nav>
        <div class="grid">
            <div id="videosBtn">Videos</div>
            <div id="artigosBtn">Artigos</div>
            <div id="profissionaisBtn">Profissionais</div>
            <div id="configBtn" onClick="config()">Configurações</div>
            <div id="sairBtn" onClick="sair()">Sair</div>
        </div>
        <div id="imgUser">
            <img src="" />
        </div>
  
    </nav>
        @RenderBody()
        <hr />

        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
</body>
</html>
